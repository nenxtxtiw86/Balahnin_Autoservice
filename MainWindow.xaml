<Window x:Class="Familiya_Autoservice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Familiya_Autoservice"
        mc:Ignorable="d"
        Title="Балахнин" Height="450" Width="800" Icon="res/service_logo.ico" FontFamily="Tw Cen MT">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/res/service_logo.png" HorizontalAlignment="Left"/>
        <TextBlock Text="Услуги автосервиса" FontSize="30"  HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Tw Cen MT" ></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack"  HorizontalAlignment="Right" Click="BtnBack_Click" FontFamily="Tw Cen MT"></Button>
      </Grid>
</Window>
